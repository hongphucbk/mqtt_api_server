<% layout('../layout/index') %>
<% block('css', `
	
  

`) %>

<!-- Content Header (Page header) -->
<section class="content-header">
  <h1>
    Parameter
    <small>List</small>
  </h1>
  <ol class="breadcrumb">
    <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
    <li class="active">Parameter</li>
  </ol>
</section>

<!-- Main content -->
<section class="content container-fluid">
	<div class="panel-heading">
		Parameter
		<span style="float: right;"><a href="/admin/parameter/add">Add new parameter</a></span>
	</div>
	<div class="row">
	  <div class="col-xs-12">
	    <div class="box">
	      <div class="box-header">
	        <h3 class="box-title">Station list</h3>
	      </div>
	      <!-- /.box-header -->
	      <div class="box-body">
	        	<table id="table1" class="table table-bordered table-striped">
						  <thead>
						    <tr>
						      <th scope="col">#</th>
						      <th scope="col">Code</th>
						      <th scope="col">Name</th>
						      <!-- <th scope="col">Map Tag</th> -->
						      <th scope="col">DataInfo</th>
						      <th scope="col">Note</th>
						      <th scope="col">Action</th>
						    </tr>
						  </thead>
						  <tbody>
						    <script type="text/javascript">
						    	var i = 1;
						    </script>
						    <% stations.forEach(function(station) { %>
						    	<tr>
							    	<td>
							    		<script type="text/javascript">
									    	document.write(i);
									    	i = i + 1;
									    </script>
									</td>
									<td> <%= station.code %> </td>
									<td> <%= station.name %> </td>
									<!-- <td> <= station.maptag %> </td> -->
									<td> <%= station.datainfo %> </td>
									<td> <%= station.note %> </td>
									<td class="text-center">
										<a class='btn btn-info btn-xs' href="/admin/parameter/edit/<%= station.id %>">
											<span class="glyphicon glyphicon-edit"></span>Edit</a>

										<% if(user.role >= 4){ %>
										<a href="/admin/parameter/delete/<%= station.id %>" class="btn btn-danger btn-xs">
											<span class="glyphicon glyphicon-remove"></span>Del</a>
										<%}%>
									</td>
								</tr>	
						    <% }); %>

						  </tbody>
						</table>
	      </div>
	      <!-- /.box-body -->
	    </div>
	    <!-- /.box -->
	  </div>
	  <!-- /.col -->
	</div>

</section>



<% block('script', `



`) -%>

